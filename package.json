{
  "name": "the-borsky",
  "version": "0.0.1",
  "description": "Shawn Borsky website",
  "main": "index.html",
  "scripts": {
    "dev:watch": "parcel watch */**/index.scss --out-dir www/assets/css/ --public-url ./  --no-hmr",
    "dev:server": "cd www && php -S localhost:1234 kirby/router.php",
    "dev": "NODE_ENV=development run-p dev:*",
    "build": "parcel build */**/index.scss --out-dir www/assets/css/ --public-url ./  --no-hmr"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Thomasorus/the-borsky.git"
  },
  "author": "Thomasorus and Sonic San",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Thomasorus/the-borsky/issues"
  },
  "homepage": "https://github.com/Thomasorus/the-borsky#readme",
  "devDependencies": {
    "parcel-bundler": "^1.12.5",
    "sass": "^1.35.1"
  },
  "dependencies": {
    "npm-run-all": "^4.1.5"
  }
}
